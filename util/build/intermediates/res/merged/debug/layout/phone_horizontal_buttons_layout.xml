<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:widget="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/phone_horizontal_btn_bg"
    android:orientation="vertical">

    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/phone_horizontal_btn_top_line" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@android:color/white"
        android:orientation="horizontal"
        android:padding="5dp">

        <ImageView
            android:id="@+id/more_left_img"
            android:layout_width="10dp"
            android:layout_height="20dp"
            android:layout_gravity="center_vertical"
            android:scaleType="centerInside"
            android:src="@mipmap/morel"
            android:visibility="invisible" />

        <com.rj.view.listview.HorizontalListView
            android:id="@+id/btns_listview"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_gravity="bottom"
            android:layout_marginBottom="0dp"
            android:layout_weight="1"
            android:divider="@android:color/transparent"
            widget:dividerWidth="5dp" />

        <ImageView
            android:id="@+id/more_right_img"
            android:layout_width="10dp"
            android:layout_height="20dp"
            android:layout_gravity="center_vertical"
            android:scaleType="centerInside"
            android:src="@mipmap/morer"
            android:visibility="invisible" />
    </LinearLayout>

</LinearLayout><!-- From: file:/F:/WISP_SAFE_ANDROID_4.0/Wisp/view/src/main/res/layout/phone_horizontal_buttons_layout.xml --><!-- From: file:/F:/WISP_SAFE_ANDROID_4.0/Wisp/util/build/intermediates/exploded-aar/Wisp/view/unspecified/res/layout/phone_horizontal_buttons_layout.xml -->